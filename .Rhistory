values = c(7,5,9,2,1,8,3,4,6,10)
iqr <- function(v) {
q1 = quantile(v,1/4,names = FALSE)
q3 = quantile(v,3/4,names = FALSE)
iqr_list=list("q1"=q1,"q3"=q3,"ext"=q3-q1)
return (iqr_list);
}

plot(ppois(x, lambda), type = "l", lwd = 2,
main = "Acumulada Poisson",
ylab = "P(X = x)", xlab = "Número de eventos", col = 'blue')
plot(qpois(seq(0, 1, 0.001), lambda), type = "l", lwd = 2,
